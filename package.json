{
  "name": "nuxt4-template",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest --project unit --outputFile=junit-unit.xml --coverage.reportsDirectory=./coverage/unit",
    "test:nuxt": "vitest --project nuxt --outputFile=junit-nuxt.xml --coverage.reportsDirectory=./coverage/nuxt"
  },
  "dependencies": {
    "@nuxt/fonts": "0.12.1",
    "@nuxt/hints": "1.0.0-alpha.5",
    "@nuxt/icon": "2.2.0",
    "@nuxt/test-utils": "3.23.0",
    "@tailwindcss/vite": "^4.1.18",
    "nuxt": "^4.2.2",
    "primevue": "^4.5.4",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tailwindcss-primeui": "^0.6.1",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.12.1",
    "@nuxt/eslint-config": "^1.12.1",
    "@vitest/coverage-v8": "^4.0.16",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "bunx --bun eslint --fix"
    ]
  }
}